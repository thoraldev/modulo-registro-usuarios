using System;
using System.Collections.Generic;
using System.Linq;
using System.Net.Http;
using System.Threading.Tasks;
using System.Windows.Forms;
using Newtonsoft.Json;

namespace Formulario_de_practica
{
    public partial class Ubicacion : Form
    {
        // URL del JSON en GitHub
        private const string UrlUbicaciones = "https://raw.githubusercontent.com/thoraldev/modulo-registro-usuarios/refs/heads/main/Recursos/ubicaciones.json";
        
        private static readonly HttpClient httpClient = new HttpClient();
        private UbicacionesData datosUbicaciones;

        public Ubicacion()
        {
            InitializeComponent();
        }

        private async void Ubicacion_Load(object sender, EventArgs e)
        {
            // Deshabilitar controles mientras carga
            comboBox_pais.Enabled = false;
            comboBox_estado.Enabled = false;
            comboBox_localidad.Enabled = false;

            await CargarUbicacionesAsync();
        }

        private async Task CargarUbicacionesAsync()
        {
            comboBox_pais.Items.Add("Cargando...");
            comboBox_pais.SelectedIndex = 0;

            try
            {
                string contenidoJson = await httpClient.GetStringAsync(UrlUbicaciones);
                datosUbicaciones = JsonConvert.DeserializeObject<UbicacionesData>(contenidoJson);

                // Cargar pa√≠ses en el ComboBox
                comboBox_pais.Items.Clear();
                comboBox_pais.DataSource = datosUbicaciones.Paises.Select(p => p.Nombre).ToList();
                comboBox_pais.SelectedIndex = -1;
                comboBox_pais.Enabled = true;
            }
            catch (Exception ex)
            {
                MessageBox.Show("No se pudieron cargar las ubicaciones: " + ex.Message, 
                    "Error de Red", MessageBoxButtons.OK, MessageBoxIcon.Error);
                comboBox_pais.Items.Clear();
                comboBox_pais.Items.Add("Error al cargar");
                comboBox_pais.SelectedIndex = 0;
            }
        }

        private void comboBox_pais_SelectedIndexChanged(object sender, EventArgs e)
        {
            // Limpiar ComboBoxes dependientes
            comboBox_estado.DataSource = null;
            comboBox_estado.Items.Clear();
            comboBox_localidad.DataSource = null;
            comboBox_localidad.Items.Clear();

            if (comboBox_pais.SelectedIndex == -1 || datosUbicaciones == null)
                return;

            string paisSeleccionado = comboBox_pais.SelectedItem.ToString();
            Pais pais = datosUbicaciones.Paises.FirstOrDefault(p => p.Nombre == paisSeleccionado);

            if (pais == null) return;

            // Caso 1: El pa√≠s tiene estados
            if (pais.Estados != null && pais.Estados.Any())
            {
                comboBox_estado.DataSource = pais.Estados.Select(estado => estado.Nombre).ToList();
                comboBox_estado.SelectedIndex = -1;
                comboBox_estado.Enabled = true;
                comboBox_localidad.Enabled = false;
            }
            // Caso 2: El pa√≠s NO tiene estados, solo localidades directas
            else if (pais.Localidades != null && pais.Localidades.Any())
            {
                comboBox_estado.Enabled = false;
                comboBox_localidad.DataSource = pais.Localidades;
                comboBox_localidad.SelectedIndex = -1;
                comboBox_localidad.Enabled = true;
            }
            else
            {
                comboBox_estado.Enabled = false;
                comboBox_localidad.Enabled = false;
            }
        }

        private void comboBox_estado_SelectedIndexChanged(object sender, EventArgs e)
        {
            comboBox_localidad.DataSource = null;
            comboBox_localidad.Items.Clear();

            if (comboBox_estado.SelectedIndex == -1 || datosUbicaciones == null)
                return;

            string paisSeleccionado = comboBox_pais.SelectedItem.ToString();
            string estadoSeleccionado = comboBox_estado.SelectedItem.ToString();

            Pais pais = datosUbicaciones.Paises.FirstOrDefault(p => p.Nombre == paisSeleccionado);
            if (pais == null) return;

            Estado estado = pais.Estados.FirstOrDefault(est => est.Nombre == estadoSeleccionado);
            if (estado == null || estado.Localidades == null) return;

            comboBox_localidad.DataSource = estado.Localidades;
            comboBox_localidad.SelectedIndex = -1;
            comboBox_localidad.Enabled = true;
        }
    }

